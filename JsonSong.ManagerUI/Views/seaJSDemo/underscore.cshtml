
@{
    ViewBag.Title = "seaJS hello world";
    Layout = "../Shared/_ContentLayout.cshtml";
}

@section head {
    <script src="~/Content/libs/seajs-2.3.0/sea-debug.js"></script>
    <script>
        seajs.config({
            base: "@HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority)",
            paths: {
                "libs": "Content/libs",
                "avalon": "Content/libs/avalon",
                "lte": "Content/libs/AdminLTE",
                "page": "Scripts/Pages"
            }
        });

        seajs.use("page/SeaJsDemo/underscoreTest");

    </script>
}
