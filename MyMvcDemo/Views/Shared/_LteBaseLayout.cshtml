@using dotless.Core.Parser.Tree
@using Suijing.Utils.ConfigTools

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>



    @if (ConfigHelper.GetConfigBool("IsLocal"))
    {
        @Url.CombresLink("baseCss")
        @Url.CombresLink("siteCss")

        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.eot" rel="stylesheet" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.ttf" rel="stylesheet" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.woff" rel="stylesheet" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.svg" rel="stylesheet" />
        @Url.CombresLink("framework")
    }
    else
    {
        <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.1/css/bootstrap.min.css">
        <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">
          <!-- Theme style -->
        <link href="~/Content/libs/AdminLTE/css/AdminLTE.css" rel="stylesheet" />

        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.eot" rel="stylesheet" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.svg" rel="stylesheet" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.ttf" rel="stylesheet" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.woff" rel="stylesheet" />


        <script src="http://cdn.staticfile.org/jquery/2.1.1-rc2/jquery.js"></script>
        <script src="http://cdn.bootcss.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
        <script src="http://cdn.staticfile.org/underscore.js/1.7.0/underscore.js"></script>
        <script src="http://cdn.bootcss.com/underscore.string/3.0.0/underscore.string.js"></script>
        <script src="http://cdn.staticfile.org/seajs/2.3.0/sea.js"></script>
        <script src="http://cdn.staticfile.org/avalon.js/1.3.4/avalon.js"></script>
        @*<script src="http://cdn.staticfile.org/avalon.js/1.3.4/modern.js"></script>*@
    }

    <!-- AdminLTE App -->
    <script src="~/Content/libs/AdminLTE/js/app.js"></script>
    @RenderSection("head", false)
</head>
<body class="skin-blue  pace-done" style="min-height: 415px;">
    @RenderBody()

    @RenderSection("foot", false)
</body>
</html>


