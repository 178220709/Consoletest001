@using dotless.Core.Parser.Tree
@using Suijing.Utils.ConfigTools


<html>
<head>
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="http://cdn.bootcss.com/font-awesome/4.2.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="http://cdn.bootcss.com/ionicons/1.5.2/css/ionicons.min.css">

    @if (ConfigHelper.GetConfigBool("IsLocal"))
    {
        @Url.CombresLink("baseCss")
        @Url.CombresLink("siteCss")

        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.eot" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.ttf" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.woff" />
        <link href="~/Content/libs/AdminLTE/bootStrap/fonts/glyphicons-halflings-regular.svg" />

        @Url.CombresLink("framework")
    }
    else
    {
        <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.1/css/bootstrap.min.css">
        <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">
        <link href="~/Content/css/style.css" rel="stylesheet" />
          <!-- Theme style -->
        <link href="~/Content/libs/AdminLTE/css/AdminLTE.css" rel="stylesheet" />

        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.eot" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.svg" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.ttf" />
        <link href="http://cdn.bootcss.com/bootstrap/3.3.1/fonts/glyphicons-halflings-regular.woff" />


        <script src="http://cdn.staticfile.org/seajs/2.3.0/sea.js"></script>


        <script src="http://cdn.staticfile.org/jquery/2.1.1-rc2/jquery.js"></script>
        <script src="http://cdn.bootcss.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
        @*<script src="http://cdn.staticfile.org/underscore.js/1.7.0/underscore.js"></script>
            <script src="http://cdn.bootcss.com/underscore.string/3.0.0/underscore.string.js"></script>*@

        @*<script src="http://cdn.staticfile.org/avalon.js/1.3.4/avalon.js"></script>*@
        @*<script src="http://cdn.staticfile.org/avalon.js/1.3.4/modern.js"></script>*@
    }

    <script>
        //seajs config的base配置文件
        seajs.config({
            base: "@HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority)",
            paths: {
                "libs": "Content/libs",
                "avalon": "Content/libs/avalon",
                "lte": "Content/libs/AdminLTE",
                "page": "Scripts/Pages"
            }
        });
    </script>
    @if (ConfigHelper.GetConfigBool("IsLocal"))
    {
        <script>
            //seajs config的本地 配置
            seajs.config({
                alias: {
                    "underscore.js": "Content/libs/underscore",
                    "jquery.js": "libs/AdminLTE/js/jquery.js"
                },
            });
        </script>
    }

    else
    {
        <script>
            seajs.config({
                alias: {
                    "underscore.js": "http://cdn.staticfile.org/underscore.js/1.7.0/underscore.js",
                    "jquery.js": "http://cdn.staticfile.org/jquery/2.1.1-rc2/jquery.js",
                },
            });
        </script>
    }


    <script>
        seajs.config({
            alias: {
               
            },
        });

    </script>



    @RenderSection("head", false)
</head>
<body class="skin-blue  pace-done" style="min-height: 415px;">
    @RenderBody()

    @RenderSection("foot", false)
</body>
</html>
