    <script>
        function SearchBaidu(formname) {
            var url = "http://www.baidu.com/baidu";
            formname.method = "get";
            if (formname.myselectvalue.value == "1") {
                document.search_form1.word.value =
                    document.search_form1.word.value + " site:xxxxx.com";
            }
            formname.action = url;
            return true;
        }


        $(function (parameters) {
            var list = [];
            var test = "";
            $("table").find("tbody").find("tr").each(function () {
                var name = $(this).find("td:eq(2)").text();
                var id = $(this).find("td:eq(1)").text();
                list.push({ id: id, name: name });
                test += ' {"' + name + '","' + id + '"},';
            });
            // console.log(test);
            // $("#mySpan").text(test);



        });
    </script>
